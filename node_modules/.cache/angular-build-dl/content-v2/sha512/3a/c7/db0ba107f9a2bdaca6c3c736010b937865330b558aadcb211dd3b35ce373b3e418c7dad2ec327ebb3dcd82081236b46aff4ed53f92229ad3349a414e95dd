ttp\r\n      .get<any>(`${environment.apiUrl}/webRoutes/getRetestDetails`)\r\n      .pipe(\r\n        map((res) => {\r\n          // localStorage.setItem('currentEmployee', JSON.stringify(user));\r\n          // this.currentEmployeeSubject.next(user);\r\n          return res;\r\n        })\r\n      );\r\n  }\r\n\r\n  \r\n  checkRetestExists(retest_status,client){\r\n    return this.http\r\n      .post<any>(`${environment.apiUrl}/webRoutes/checkRetestExists`,{retest_status,client})\r\n      .pipe(\r\n        map((res) => {\r\n          // localStorage.setItem('currentEmployee', JSON.stringify(user));\r\n          // this.currentEmployeeSubject.next(user);\r\n          return res;\r\n        })\r\n      );\r\n  }\r\n\r\n  setRetestPayment(retestData,authToken,client){\r\n    return this.http\r\n      .post<any>(`${environment.apiUrl}/webRoutes/setRetestPayment`,{retestData,authToken,client})\r\n      .pipe(\r\n        map((res) => {\r\n          // localStorage.setItem('currentEmployee', JSON.stringify(user));\r\n          // this.currentEmployeeSubject.next(user);\r\n          return res;\r\n        })\r\n      );\r\n  }\r\n\r\n  updateRetestPayment(retestData,authToken,client){\r\n    return this.http\r\n      .post<any>(`${environment.apiUrl}/webRoutes/updateRetestPayment`,{retestData,authToken,client})\r\n      .pipe(\r\n        map((res) => {\r\n          // localStorage.setItem('currentEmployee', JSON.stringify(user));\r\n          // this.currentEmployeeSubject.next(user);\r\n          return res;\r\n        })\r\n      );\r\n  }\r\n\r\n  deleteRetestPayment(retestData){\r\n    return this.http\r\n      .post<any>(`${environment.apiUrl}/webRoutes/deleteRetestPayment`,{retestData})\r\n      .pipe(\r\n        map((res) => {\r\n          // localStorage.setItem('currentEmployee', JSON.stringify(user));\r\n          // this.currentEmployeeSubject.next(user);\r\n          return res;\r\n        })\r\n      );\r\n  }\r\n\r\n \r\n\r\n}\r\n"]}