Change.value;\r\n  }\r\n\r\n  constructor(private http: HttpClient) {\r\n  }\r\n\r\n  setLicenseDetails(formData){\r\n    return this.http\r\n    .post<any>(`${environment.apiUrl}/webRoutes/setLicenseDetails`,formData)\r\n    .pipe(\r\n      map((res) => {\r\n        // localStorage.setItem('currentUser', JSON.stringify(user));\r\n        // this.currentUserSubject.next(user);\r\n        return res;\r\n      })\r\n    );\r\n  }\r\n\r\n  getLicenseListPerClient(client){\r\n    return this.http\r\n    .post<any>(`${environment.apiUrl}/webRoutes/getLicenseListPerClient`,{client})\r\n    .pipe(\r\n      map((res) => {\r\n        this.isTblLoading = false;\r\n        this.dataChange.next(res);\r\n        return res;\r\n      })\r\n    );\r\n  }\r\n\r\n  deleteLicenseDetails(rowToDelete,authToken,client){\r\n    return this.http\r\n    .post<any>(`${environment.apiUrl}/webRoutes/deleteLicenseDetails`,{rowToDelete,authToken,client})\r\n    .pipe(\r\n      map((res) => {\r\n        return res;\r\n      })\r\n    );\r\n  }\r\n\r\n  getLicenseToEdit(licenseId,client){\r\n    return this.http\r\n    .post<any>(`${environment.apiUrl}/webRoutes/getLicenseToEdit`,{licenseId,client})\r\n    .pipe(\r\n      map((res) => {\r\n        return res;\r\n      })\r\n    );\r\n  }\r\n\r\n  updateLicenseDetails(formData){\r\n    return this.http\r\n    .post<any>(`${environment.apiUrl}/webRoutes/updateLicenseDetails`,formData)\r\n    .pipe(\r\n      map((res) => {\r\n        // localStorage.setItem('currentUser', JSON.stringify(user));\r\n        // this.currentUserSubject.next(user);\r\n        return res;\r\n      })\r\n    );\r\n  }\r\n\r\n  getClientCodeForFolder(client){\r\n    return this.http\r\n    .post<any>(`${environment.apiUrl}/webRoutes/getClientCodeForFolder`,{client})\r\n    .pipe(\r\n      map((res) => {\r\n        return res;\r\n      })\r\n    );\r\n  }\r\n\r\n\r\n}\r\n"],"sourceRoot":"webpack:///"}