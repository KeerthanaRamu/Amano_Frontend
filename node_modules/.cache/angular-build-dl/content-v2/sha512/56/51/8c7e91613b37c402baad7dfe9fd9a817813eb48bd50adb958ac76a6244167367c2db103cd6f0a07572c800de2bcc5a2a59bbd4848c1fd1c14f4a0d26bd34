lar/core';\r\nimport { Routes, RouterModule } from '@angular/router';\r\nimport { ProfileComponent } from './profile.component'; \r\nconst routes: Routes = [\r\n  {\r\n    path: '',\r\n    component: ProfileComponent,\r\n  },\r\n];\r\n@NgModule({\r\n  imports: [RouterModule.forChild(routes)],\r\n  exports: [RouterModule],\r\n})\r\nexport class ProfileRoutingModule {}\r\n","import { Component, OnInit } from '@angular/core';\r\nimport { AuthService } from 'src/app/core/service/auth.service';\r\nimport { ToastrService } from 'ngx-toastr';\r\n\r\nimport {\r\n  FormGroup,\r\n  FormBuilder,\r\n  FormControl,\r\n  Validators,\r\n} from '@angular/forms';\r\n\r\n\r\n@Component({\r\n  selector: 'app-profile',\r\n  templateUrl: './profile.component.html',\r\n  styleUrls: ['./profile.component.scss']\r\n})\r\nexport class ProfileComponent implements OnInit {\r\n  userDetails: any;\r\n  newLogo;\r\n  securityForm:FormGroup;\r\n  accountsForm:FormGroup;\r\n\r\n  constructor(private fb: FormBuilder,private authService: AuthService,private toastrService: ToastrService) {}\r\n  ngOnInit() {\r\n\r\n    this.securityForm = this.fb.group({\r\n      user_name: ['',Validators.required],\r\n      new_username:['',Validators.required],\r\n      password:['',Validators.required],\r\n      current_password: ['',Validators.required],\r\n      new_password: ['',Validators.required],\r\n    });\r\n\r\n    this.accountsForm = this.fb.group({\r\n      name: ['',Validators.required],\r\n      mobile_no: ['',Validators.required],\r\n      email_id: ['',Validators.required],\r\n      profile:['']\r\n    });\r\n\r\n    this.authService.currentUserValue(sessionStorage.currentUser)\r\n    .subscribe(res=>{\r\n      const userRole = res['role'];\r\n      this.userDetails = res;\r\n      this.securityForm.patchValue({\r\n        user_name:this.userDetails.user_name,\r\n        new_username:this.userDetails.user_name,\r\n        password:this.userDetails.password\r\n      });\r\n\r\n\r\n      this.accountsForm.patchValue({\r\n        name:this.userDetails.name,\r\n        mobile_no: this.userDetails.mobile_no,\r\n        email_id: this.userDetails.email_id,\r\n        profile:this.userDetails.img\r\n      })\r\n    })\r\n\r\n  }\r\n\r\n  onFileChange(event) {\r\n    const reader = new FileReader();\r\n    if (event.target.files && event.target.files.length) {\r\n      const [file] = event.target.files;\r\n      reader.readAsDataURL(file);\r\n      reader.onload = () => {\r\n        this.newLogo=reader.result;\r\n      }\r\n    }\r\n  }\r\n\r\n  onFileChangeProfile(event) {\r\n    const reader = new FileReader();\r\n    if (event.target.files && event.target.files.length) {\r\n      const [file] = event.target.files;\r\n      reader.readAsDataURL(file);\r\n      reader.onload = () => {\r\n       this.accountsForm.patchValue({\r\n        profile:reader.result\r\n       })\r\n      }\r\n    }\r\n  }\r\n\r\n  updateClientLogo(){\r\n    if(this.newLogo){\r\n      this.authService.updateClientLogo(sessionStorage.client,this.newLogo)\r\n      .subscribe(res=>{\r\n          this.authService.currentUserSubject.next(true);\r\n          this.toastrService.success(\"Updated Successfully!!!\")\r\n      })\r\n    }\r\n  }\r\n\r\n  updateUserCredentials(){\r\n  if(this.securityForm.value.password == this.securityForm.value.current_password){\r\n      this.authService.updateUserCredentials(this.securityForm.value,sessionStorage.currentUser)\r\n      .subscribe(res=>{\r\n        if(res['status'] == 'Success'){\r\n          this.authService.currentUserSubject.next(true);\r\n          this.toastrService.success(\"Updated Successfully!!!\")\r\n        }else{\r\n          this.toastrService.error('User Name Already Exists!!')\r\n        }\r\n          \r\n      })\r\n  }else{\r\n    this.toastrService.error(\"Current Password is not Correct!!\")\r\n  }\r\n    \r\n  }\r\n\r\n  updateAccountDetails(){\r\n    this.authService.updateAccountDetails(this.accountsForm.value,sessionStorage.currentUser)\r\n      .subscribe(res=>{\r\n          this.authService.currentUserSubject.next(true);\r\n          this.toastrService.success(\"Updated Successfully!!!\");\r\n          this.authService.currentUserValue(sessionStorage.currentUser)\r\n          .subscribe(res=>{\r\n            const userRole = res['role'];\r\n            this.userDetails = res;\r\n            this.securityForm.patchValue({\r\n              user_name:this.userDetails.user_name,\r\n              new_username:this.userDetails.user_name,\r\n              password:this.userDetails.password\r\n            });\r\n\r\n\r\n            this.accountsForm.patchValue({\r\n              name:this.userDetails.name,\r\n              mobile_no: this.userDetails.mobile_no,\r\n              email_id: this.userDetails.email_id,\r\n              profile:this.userDetails.img\r\n            })\r\n          })\r\n    })\r\n  }\r\n  \r\n}\r\n","<section class=\"content\">\r\n  <div class=\"container-fluid\">\r\n    <div class=\"block-header\">\r\n      <div class=\"row\">\r\n        <div class=\"col-xs-12 col-sm-12 col-md-12 col-lg-12\">\r\n          <ul class=\"breadcrumb breadcrumb-style \">\r\n            <li class=\"breadcrumb-item\">\r\n              <h4 class=\"page-title\">Profile</h4>\r\n            </li>\r\n          </ul>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <!-- Your content goes here  -->\r\n    <div class=\"row clearfix\">\r\n      <div class=\"col-lg-4 col-md-12\">\r\n        <div class=\"card\">\r\n          <div class=\"m-b-20\">\r\n            <div class=\"contact-grid\">\r\n              <div class=\"profile-header bg-dark\">\r\n                <div class=\"user-name\">{{userDetails.name}}</div>\r\n                <div class=\"name-center\">{{userDetails.role}}</div>\r\n              </div>\r\n              <img src=\"{{userDetails.img ? userDetails.img :'assets/images/nouser.png'}}\" style=\"width: 100%;height: 100px;\" class=\"user-img\" alt=\"\">\r\n              <!-- <p>\r\n